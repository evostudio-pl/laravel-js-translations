{
    "name": "evostudio/laravel-js-translations",
    "description": "Use your Laravel translations in JavaScript",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dawid Herrlein",
            "email": "dawid.herrlein@evostudio.pl"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0",
        "illuminate/view": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "EvoStudio\\JsTranslations\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EvoStudio\\JsTranslations\\JsTranslationsServiceProvider"
            ],
            "aliases": {}
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
